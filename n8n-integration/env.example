# DecentraMind N8N Integration Environment Configuration

# Application Settings
NODE_ENV=development
PORT=3001
LOG_LEVEL=info

# N8N Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_USER_MANAGEMENT_DISABLED=true
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123
N8N_ENCRYPTION_KEY=decentramind-n8n-key-2024

# Webhook Configuration
WEBHOOK_URL=http://localhost:3001
WEBHOOK_SECRET=decentramind-webhook-secret

# Database Configuration
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB=decentramind_n8n
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=password123

# Redis Configuration
REDIS_PASSWORD=redis123

# Solana Configuration
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_API_KEY=your_solana_api_key_here

# Frontend Configuration
FRONTEND_URL=http://localhost:3000

# Authentication
JWT_SECRET=decentramind-jwt-secret-2024
JWT_EXPIRY=24h

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# External API Keys
EHR_API_KEY=your_ehr_api_key_here
INSURANCE_API_KEY=your_insurance_api_key_here
ACCOUNTING_API_KEY=your_accounting_api_key_here
BANKING_API_KEY=your_banking_api_key_here
REGULATORY_API_KEY=your_regulatory_api_key_here
DOCUMENT_API_KEY=your_document_api_key_here

# Notification Services
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here

# Test Configuration
TEST_TELEGRAM_CHAT_ID=your_test_chat_id
TEST_DISCORD_CHANNEL_ID=your_test_channel_id
TEST_EMAIL_ADDRESS=test@example.com

# Admin Configuration
ADMIN_TELEGRAM_CHAT_IDS=admin_chat_id_1,admin_chat_id_2
ADMIN_DISCORD_CHANNEL_IDS=admin_channel_id_1,admin_channel_id_2

# Monitoring
GRAFANA_PASSWORD=admin123

# Production Overrides (uncomment for production)
# NODE_ENV=production
# N8N_PROTOCOL=https
# LOG_LEVEL=warn
# MONGODB_URI=mongodb://mongodb:27017
# WEBHOOK_URL=https://your-domain.com
# FRONTEND_URL=https://your-frontend-domain.com
